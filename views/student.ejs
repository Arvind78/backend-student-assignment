<!-- index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Student</title>
</head>
<body>
  <h1><%= heading %></h1>
  <ul>
    <% data.forEach(item => { %>
      <div>
      <h3>Student id : <%= item.id%></h3>
      <p> Name: <%= item.name%></p>
      <p> GPA: <%= item.gpa%></p>
    </div> <br> 

    <% }); %>
  </ul>
</body>
</html>
